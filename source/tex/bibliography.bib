	pages = {601--611},
	file = {Chowdhury (2014) - Michaelis–Menten at 100 and allosterism at 50.pdf:C\:\\Users\\panm\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\eexbckdj.default\\zotero\\storage\\IDGEGW4W\\Chowdhury (2014) - Michaelis–Menten at 100 and allosterism at 50.pdf:application/pdf;Snapshot:C\:\\Users\\panm\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\eexbckdj.default\\zotero\\storage\\W57P98DP\\abstract.html:text/html}
