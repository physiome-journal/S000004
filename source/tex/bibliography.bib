	keywords = {biochemical networks, modelling, ordinary differential equations, Petri nets, signal transduction, Systems Biology},
	pages = {404--421},
	file = {Breitling et al (2008) - A structured approach for the engineering of biochemical network models,.pdf:C\:\\Users\\panm\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\eexbckdj.default\\zotero\\storage\\JUFBVW38\\Breitling et al (2008) - A structured approach for the engineering of biochemical network models,.pdf:application/pdf;Snapshot:C\:\\Users\\panm\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\eexbckdj.default\\zotero\\storage\\UXC78DS2\\404.html:text/html}
