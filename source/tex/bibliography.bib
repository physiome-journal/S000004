	pmid = {24920110},
	keywords = {Plant biology},
	pages = {20140362},
	file = {Breuer et al (2014) - Quantitative analyses of the plant cytoskeleton reveal underlying.pdf:C\:\\Users\\panm\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\eexbckdj.default\\zotero\\storage\\KXCDK5PP\\Breuer et al (2014) - Quantitative analyses of the plant cytoskeleton reveal underlying.pdf:application/pdf;Snapshot:C\:\\Users\\panm\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\eexbckdj.default\\zotero\\storage\\W7P8M4Z5\\20140362.html:text/html}
