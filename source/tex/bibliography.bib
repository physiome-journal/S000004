	author = {Southern, James and Pitt-Francis, Joe and Whiteley, Jonathan and Stokeley, Daniel and Kobashi, Hiromichi and Nobes, Ross and Kadooka, Yoshimasa and Gavaghan, David},
	month = jan,
	year = {2008},
	keywords = {Cardiac modelling, ion channel, Mathematical modelling, Multi-physics, multi-scale, simulation},
